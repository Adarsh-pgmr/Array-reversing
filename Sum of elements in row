#include<stdio.h>

void main ()

{

  int i,row,col,j,s=0;

  int a[5][5];

  printf("Enter the number of rows\n");

  scanf("%d",&row);

  printf("Enter the number of column\n");

  scanf("%d",&col);

  for(i=0;i<row;i++)

  {

   printf("Enter the elements in row %d\n");

   for(j=0;j<col;j++)

    scanf("%d",&a[i][j]);

   }

  printf("\nThe matrice is of A \n");

  for(i=0;i<row;i++)

  {

   printf("\n");

   for(j=0;j<col;j++)

    printf("%d\t",a[i][j]);

   }

  for(i=0;i<row;i++)

  {

  // printf("\nEnter the elements in row %d\n");

   for(j=0;j<col;j++)

    s=s+a[i][j];

   

  printf("\nThe sum of elements in row %d is %d\n",i,s);

  s=0;

  }
