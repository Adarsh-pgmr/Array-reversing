#include<stdio.h>

void main ()

{

  int a[2][3]={{1,2,3},{3,4,5}},b[3][2];

  int i,j;

 

  printf("The initial matrice is \n");

  for(i=0;i<2;i++)

  {

    printf("\n");

    for(j=0;j<3;j++)

    {

     printf("%d\t",a[i][j]);

    }

  }

  printf("The transpose of matrix is \n");

    for(i=0;i<3;i++)

  {

    printf("\n");

    for(j=0;j<2;j++)

    {

     printf("%d\t",a[j][i]);

    }

  }

}
