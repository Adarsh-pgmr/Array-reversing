#include<stdio.h>

int main()

{

   char s1[20],s2[20];

   int i=0,j=0;

   puts("Enter the string s1");

   gets(s1);

   while(s1[i]!='\0')   

   i++;

   i--;

   while(i>=0)

   {

      s2[j]=s1[i];

      i--;

      j++;

   }

    s2[j]='\0';

    printf("The reverse string is %s",s2);

   

    return 0;

}

