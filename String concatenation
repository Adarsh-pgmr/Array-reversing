#include<stdio.h>

void main()

{

   int i,j;

   char s1[20],s2[20];

   puts("\nEnter the first string s1");

   gets(s1);

   puts("\nEnter the second string s2");

   gets (s2);

   for(i=0;s1[i]!='\0';i++);

   for(j=0;s2[j]!='\0';i++)

   {

      s1[i]=s2[j];

      j++;

   }

    s2[j]='\0';

    printf("\n%s",s1);

 }
